unit uEmpresaModel;

interface

type
  TEmpresa = class
  private
    FCodigo: Integer;
    FRSocial: string;

    procedure SetCodigo(const Value: Integer);
    procedure SetRSocial(const Value: string);
    procedure Carregar;

  public
    property Codigo: Integer read FCodigo write SetCodigo;
    property RSocial: string read FRSocial write SetRSocial;
  end;

implementation

{ TEmpresa }

procedure TEmpresa.Carregar;
begin

end;

procedure TEmpresa.SetCodigo(const Value: Integer);
begin
  FCodigo := Value;
end;

procedure TEmpresa.SetRSocial(const Value: string);
begin
  FRSocial := Value;
end;

end.
